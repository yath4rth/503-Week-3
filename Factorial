public class Factorial 
{
	public static void main(String args[])
	{
		int factorial = 1;
		System.out.print("10! is equal to: ");
		for (int i=1; i<=10; i++)
		{
			factorial *=i;	
		}
		for(int j=1; j<10; j++)
				{
					System.out.print(j+" x ");
				}
		System.out.print(10);		
		System.out.print(" = "+ factorial);
	}
}
